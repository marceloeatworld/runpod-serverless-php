{
    "name": "marceloeatworld/runpod-serverless-php",
    "description": "A PHP client for the RunPod Serverless API",
    "keywords": ["runpod", "serverless", "php", "api-client"],
    "license": "MIT", 
    "authors": [
        {
            "name": "Marcelo Pereira",
            "email": "diagngo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "saloonphp/saloon": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MarceloEatWorld\\RunPod\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
 }